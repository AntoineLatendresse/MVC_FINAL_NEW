@model test.Models.Users
@{
    ViewBag.Title = "Lister";
}

<style>

    
    span {
        border-radius: 25px;
        border: 2px solid black;
        padding: 4px;
        background-color: lightgreen;
        font-family: monospace;
    }

    img:hover {
        border: 2px solid black;
    }

    img {
        padding: 2px;
        width: 200px;
        height: 200px;
    }

</style>




<h2>Lister</h2>

<script type="text/javascript">
    var timer;
    function ChargeVuePartielle() {
        $.ajax({
            url: '@Url.Action("PartielMarche")',
            type: 'GET',
            dataType: 'html',
            success: function (result) {
                $('#contributors').html(result);
            }
        });
    }
    $
    ChargeVuePartielle();
    $(function () {
        timer = window.setInterval("ChargeVuePartielle()", 3000);
    });



</script>

<div name="abc">
    @DateTime.Now
</div>


<table id="listeUser" class="CSSTableGenerator">
    <tr>
        @while (Model.Next())
        {
            <td>
                @Model.UserName
                
            </td>
            <td>
                @Model.GetRank()
            </td>
            if ((Model.ID % 5) == 0)
            {
                @Html.Raw("</tr>");
            @Html.Raw("<tr>");
            }
        }
    </tr>
</table>